<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox id="manifestVBox" fx:id="manifestVBox" styleClass="page-preview" stylesheets="@../styles/styles.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane hgap="10.0" vgap="10.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" percentWidth="5.0" />
          <ColumnConstraints hgrow="SOMETIMES" percentWidth="45.0" />
          <ColumnConstraints hgrow="SOMETIMES" percentWidth="45.0" />
            <ColumnConstraints hgrow="SOMETIMES" percentWidth="5.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints percentHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints percentHeight="50.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="routeLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="page-header" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Label>
            <Label fx:id="stopLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="page-header" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
            <Label fx:id="caseTrailerPositionLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1" />
         </children>
         <opaqueInsets>
            <Insets bottom="20.0" />
         </opaqueInsets>
      </GridPane>
      <TableView fx:id="casesTable" focusTraversable="false" maxHeight="250.0">
        <columns>
            <TableColumn fx:id="itemIdColumn" resizable="false" text="WRIN" />
          <TableColumn fx:id="itemDescriptionColumn" resizable="false" text="DESCRIPTION" />
          <TableColumn fx:id="casesColumn" resizable="false" text="CASES" />
            <TableColumn fx:id="stopColumn" resizable="false" text="STOP" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </TableView>
      <Label fx:id="cartTotalLabel" />
   </children>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</VBox>
