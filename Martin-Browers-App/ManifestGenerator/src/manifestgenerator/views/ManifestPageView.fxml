<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="manifestPage" alignment="CENTER_LEFT" maxHeight="-Infinity" stylesheets="@../styles/styles.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane fx:id="headerGrid">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="routeLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
            <Label fx:id="stopLabel" layoutX="10.0" layoutY="17.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" />
            <Label fx:id="caseTrailerPositionLabel" alignment="CENTER" layoutX="310.0" layoutY="17.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" textAlignment="CENTER" GridPane.columnSpan="2" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <TableView fx:id="casesTable" focusTraversable="false">
         <columns>
            <TableColumn fx:id="itemIdColumn" editable="false" sortable="false" text="WRIN" />
            <TableColumn fx:id="itemDescriptionColumn" editable="false" minWidth="150.0" prefWidth="-1.0" sortable="false" text="DESCRIPTION" />
            <TableColumn fx:id="casesColumn" editable="false" sortable="false" text="CASES" />
            <TableColumn fx:id="stopColumn" editable="false" sortable="false" text="STOP" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </TableView>
      <Label fx:id="cartTotalLabel" />
   </children>
</VBox>
